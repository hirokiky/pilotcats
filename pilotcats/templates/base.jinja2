    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    {% block staticfiles %}
    {% endblock %}
    <title>{% block title %}{% endblock %}</title>
</head>
<body>
{% block relbar1 %}{% endblock %}
<div class="document">
    <div class="documentwrapper">
        <div class="bodywrapper">
